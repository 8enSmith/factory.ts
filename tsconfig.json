{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2015",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": true,
    "noEmitHelpers": false,
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "*" : [ "*"]
    }
  }
}
